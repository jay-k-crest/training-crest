    "url = \"https://jsonplaceholder.typicode.com/todos/1\"\n",
    "\n",
    "\n",
    "response = requests.get(url)\n",
    "\n",
    "\n",
    "data = response.json()\n",
    "\n",
    "\n",
    "print(\"Full JSON response:\")\n",
    "print(data)\n",
    "\n",
    "print(\"\\nKey - Value pairs:\")\n",
    "\n",
    "for key, value in data.items():\n",
    "    print(f\"{key} : {value}\")\n"
