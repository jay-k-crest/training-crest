   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "data = np.random.rand(4, 3)\n",
    "df = pd.DataFrame(data, columns=['X', 'Y', 'Z'])\n",
    "df['Expanding_Std'] = df['X'].expanding().std()\n",
    "print(df) "
   ]
  }
